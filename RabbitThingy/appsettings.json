{
  "RabbitMqConfig": {
    "HostName": "localhost",
    "Port": 5672,
    "UserName": "admin",
    "Password": "admin"
  },
  "InputQueues": {
    "Queue1": "queue1",
    "Queue2": "queue2"
  },
  "OutputExchange": {
    "Name": "exchange",
    "RoutingKey": ""
  },
  "OutputFileSizeLimit": 1000,
  "Batching": {
    "TimeoutSeconds": 5,
    "MaxMessages": 10
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss} {Level:u3}] {Message:lj} <s:{SourceContext}>{NewLine}{Exception}"
        }
      }
    ]
  }
}